namespace connection_db
{
    public partial class Form1 : Form
    {
        public Form1()
        {
            InitializeComponent();
        }

        private void Form1_Load(object sender, EventArgs e)
        {
            SqlConnection cn = new SqlConnection(@"Data Source=.\SQLEXPRESS;AttachDbFilename=kishan.mdf;Integrated Security=True;Connect Timeout=30;User Instance=True");
            string sql = "insert into kb values ('kishan')";
            SqlDataAdapter da = new SqlDataAdapter(sql,cn);
            DataSet ds = new DataSet();
            da.Fill(ds);
        }
    }
